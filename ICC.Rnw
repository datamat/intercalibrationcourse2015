zuz <- aggregate(bar[bar$sheet==oo,],by=list(bar$picture[bar$sheet==oo]),
                 mean,na.rm=TRUE)
zuz <- zuz[,-1]

par(mfrow=c(1,6),mar=c(4,1,5,1),oma=c(2,5,0,2))

ii <- which(names(hoi)=="CHdefol_diff")
zuz <- zuz[order(zuz[,ii]),]
# head(zuz$picture)
hoi <- hoi[order(factor(hoi$picture,levels=c(zuz$picture))),]
